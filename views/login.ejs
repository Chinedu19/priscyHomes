<!doctype html>
<html class="no-js" lang="zxx">

<%- include('./partials/head', {title: 'Login' }); %>

    <body>

        <!-- Add your site or application content here -->
        <%- include('./partials/nav'); %>
            <!-- Body main wrapper start -->
            <div class="body-wrapper">

                <div class="ltn__utilize-overlay"></div>


                <!-- LOGIN AREA START -->
                <div class="ltn__login-area pb-65">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="section-title-area text-center">
                                    <h1 class="section-title">Sign In <br>To Your Account</h1>
                                    <p>Interactive web platform for our realtors</p>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="account-login-inner">
                                    <form action="login-form" class="ltn__form-box contact-form-box">
                                        <input type="email" name="email" placeholder="Email*">
                                        <input type="password" name="password" placeholder="Password*">
                                        <div class="btn-wrapper mt-0">
                                            <button class="theme-btn-1 btn btn-block" type="submit">SIGN IN</button>
                                        </div>
                                        <div class="go-to-btn mt-20">
                                            <a href="#" title="Forgot Password?" data-bs-toggle="modal"
                                                data-bs-target="#ltn_forget_password_modal"><small>FORGOTTEN YOUR
                                                    PASSWORD?</small></a>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="account-create text-center pt-50">
                                    <h4>DON'T HAVE AN ACCOUNT?</h4>
                                    <p></p>
                                    <div class="btn-wrapper">
                                        <a href="register.html" class="theme-btn-1 btn black-btn">CREATE ACCOUNT</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- LOGIN AREA END -->


                <%- include('./partials/footer'); %>
            </div>
            <!-- Body main wrapper end -->

            <!-- All JS Plugins -->
            <script src="js/plugins.js"></script>
            <!-- Contact Form -->
            <script src="js/contact.js"></script>
            <!-- Main JS -->
            <script src="js/main.js"></script>

    </body>

</html>